    <script type="text/javascript">
        $(document).ready(function () {

            function setTotalNotification(totalNotif) {
                console.log(totalNotif);
                if (totalNotif.length != 0) {
                    $("#messages").html(totalNotif.length);
                    $("#messages").css("color", "red");

                    for (var i = 0; i < totalNotif.length; i++) {
                        var url = "/" + totalNotif[i].URL + "/" + totalNotif[i].ObjectId;

                        var startDate = new Date(totalNotif[i].NotificationDate);
                        console.log("startDate" + startDate + "    not :" + totalNotif[i].NotificationDate)
                        // Do your operations
                        var endDate = new Date();



                        var diffMs = endDate.getTime() - startDate.getTime();
                        var diffDays = Math.floor(diffMs / 86400000); // days
                        var diffHrs = Math.floor((diffMs % 86400000) / 3600000); // hours
                        var diffMins = Math.round(((diffMs % 86400000) % 3600000) / 60000); // minutes

                        x = "";

                        if (diffDays > 0) {
                            x = diffDays + " days " + diffHrs + " HH " + diffMins + " MM";

                        } else if (diffHrs > 0) {
                            x = diffHrs + " HH " + diffMins + " MM";
                        } else if (diffHrs > 0) {
                            x = diffMins + " MM";
                        } else {
                            x = " Now ";
                        }




                        console.log("endDate" + endDate)
                        st = "<li  class='NoyificationLink'  id='" + totalNotif[i].id + "' >" +
                            "<a href='" + url + "'" +
                            "<div>" +
                            "<i class='fa fa-comment fa-fw'  ></i>" + totalNotif[i].Title +
                            "<span class='pull-right text-muted small' > " + x + " </span >" +
                            "</div>" +
                            "</a>" +
                            "</li>" +
                            "<li class='divider'></li>";
                        $("#dropdown2").append(st);

                    }
                }



            }

            var hub = $.connection.notificationHub;

            //Client Call
            hub.client.broadcaastNotif = function (totalNotif) {

                console.log("broadcaast Notif");
                setTotalNotification(totalNotif)
            };


            //$.connection.hub.start().done(function () { });

            $.connection.hub.start()
                .done(function () {
                    console.log("Hub Connected!" + $.connection.hub.id);

                    //Server Call
                    hub.server.getNotification();

                })
                .fail(function () {
                    console.log("Could not Connect!");
                });
        });
    </script>
    <script>
