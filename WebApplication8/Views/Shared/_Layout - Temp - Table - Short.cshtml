            <script type="text/javascript">

                var _gaq = _gaq || [];
                _gaq.push(['_setAccount', 'UA-36251023-1']);
                _gaq.push(['_setDomainName', 'jqueryscript.net']);
                _gaq.push(['_trackPageview']);

                (function () {
                    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
                })();



                function openChildMaterial() {
                    showModalPopUp1('/Materials/create');

                }

                function openChildLPO() {
                    showModalPopUp1('/Lpoes/Index');

                }

                function openChildStock() {
                    showModalPopUp1('/StockIssues/Index');

                }

                function openChildWraehouse() {
                    showModalPopUp1('/Warehouses/Index');

                }

                function openChildUnits() {
                    showModalPopUp1('/Units/Index');

                }
                function openChildSupplier() {
                    showModalPopUp1('/Suppliers/Index');

                }
                function openChildCuttingSheet() {
                    showModalPopUp1('/CuttingSheets/Index');

                }

                function openChildProject() {
                    showModalPopUp1('/Projects/Index');

                }

            </script>

            <script type="text/javascript">

                var popUpObj;

                function showModalPopUp1(url) {

                    popUpObj = window.open(url,

                        //"ModalPopUp",

                        //"toolbar=no," +

                        //"scrollbars=no," +

                        "location=no," +

                        "statusbar=no," +

                        "menubar=no," +

                        "resizable=0," +

                        "width=800," +

                        "height=400," +

                        "left = 490," +

                        "top=300"

                    );

                    popUpObj.focus();

                    LoadModalDiv();

                }

            </script>
